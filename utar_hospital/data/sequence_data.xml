<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="seq_patient" model="ir.sequence">
            <field name="name">Patient Sequence</field>
            <field name="code">hospital.patient</field>
            <field name="prefix">P</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_appointment" model="ir.sequence">
            <field name="name">Appointment Sequence</field>
            <field name="code">hospital.appointment</field>
            <field name="prefix">APM</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_doctor" model="ir.sequence">
            <field name="name">Doctor Sequence</field>
            <field name="code">hospital.doctor</field>
            <field name="prefix">DOC</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_nurse" model="ir.sequence">
            <field name="name">Nurse Sequence</field>
            <field name="code">hospital.nurse</field>
            <field name="prefix">NUR</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!--For Room Section-->
        <record id="seq_room" model="ir.sequence">
            <field name="name">Room Sequence</field>
            <field name="code">hospital.room</field>
            <field name="prefix">RM</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_ward" model="ir.sequence">
            <field name="name">Ward Sequence</field>
            <field name="code">hospital.ward</field>
            <field name="prefix">W</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>


</odoo>